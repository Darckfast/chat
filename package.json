{
  "name": "src",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --delete-dir-on-start --out-dir dist --copy-files --no-copy-ignored && rm -rf dist/web && (cd src/web && ng build)",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/app.ts",
    "start": "node dist/app.js",
    "test": "jest"
  },
  "dependencies": {
    "@types/socket.io": "^2.1.11",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jose": "^3.1.0",
    "socket.io": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
